<model xmlns="http://cxf.apache.org/jaxrs">
 <resource name="org.example.customers.CustomerService" path="customerservice">
    
    <operation name="updateCustomer" verb="PUT" path="/customer"
               consumes="application/xml">
        <param name="newCustomer" type="REQUEST_BODY"/>
    </operation>
    
    <operation name="getCustomerByName" verb="GET" path="/customer"
               produces="application/xml">
        <param name="name" type="QUERY"/>
    </operation> 
 </resource>
 
</model>
